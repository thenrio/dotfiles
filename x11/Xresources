URxvt.geometry:         74x27
URxvt.intensityStyles:  false
URxvt.perl-ext-common:  fullscreen
URxvt.transparent:      true

#include ".Xresources.d/fonts"
#include ".Xresources.d/colors"
#include ".Xresources.d/keysym"
